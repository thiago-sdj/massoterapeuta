<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#B5CFC8">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:gravity="center">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Data"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>

            <TextView
                android:id="@+id/itens_sessao_selecionada_data"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Data"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>



            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Hora início"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>
            <TextView
                android:id="@+id/itens_sessao_selecionada_horaInicio"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Hora Início"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/itens_sessao_selecionada_data"
                android:gravity="center"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Hora fim"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>
            <TextView
                android:id="@+id/itens_sessao_selecionada_horaFim"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Hora Fim"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@+id/itens_cliente_selecionado_telefone"
                android:gravity="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Peso"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>
            <TextView
                android:id="@+id/itens_sessao_selecionada_peso"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Peso"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@id/itens_sessao_selecionada_horaFim"
                android:gravity="center"/>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Status"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>

            <TextView
                android:id="@+id/itens_sessao_selecionada_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Status"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@id/itens_sessao_selecionada_peso"
                android:gravity="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Cliente"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>

            <TextView
                android:id="@+id/itens_sessao_selecionada_cliente"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Cliente"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@id/itens_sessao_selecionada_peso"
                android:gravity="center"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/black"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Endereço"
                android:textColor="#463F3A"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="center"/>

            <TextView
                android:id="@+id/itens_sessao_selecionada_endereco"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:layout_marginStart="100dp"
                android:layout_marginTop="35dp"
                android:text="Endereço"
                android:textColor="#463F3A"
                android:textSize="20sp"
                app:layout_constraintTop_toBottomOf="@id/itens_sessao_selecionada_peso"
                android:gravity="center"/>

        </LinearLayout>




</androidx.constraintlayout.widget.ConstraintLayout>